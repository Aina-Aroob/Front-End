{
    "version": 2,
    "builds": [
        {
            "src": "*.html",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/detect",
            "dest": "https://your-backend-url.com/detect"
        },
        {
            "src": "/(.*)",
            "dest": "/$1"
        }
    ]
} 